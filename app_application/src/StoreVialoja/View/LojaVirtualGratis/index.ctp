<script type="text/javascript">
$(window).on('load', function() {
  if (Cookies.get('popup_shown')) return;
  
  var $popup, interval_id, qs;

  // If user already focused on signup input, don't show popup
  $('.signup_input').one('focus', function(){ Cookies.set('popup_shown', true); })
  
  // Parse URL query
  qs = querystring.parse();

  // TODO: Remove this, temporarly enable popup for all users
  qs.pop = 't';
  
  if (qs.pop === 't' && !Cookies.get('popup_timestamp')) { Cookies.set('popup_timestamp', new Date().getTime()); }
  if (!Cookies.get('popup_timestamp')) return;

  $popup = $("#start_now_popup");
  $popup.on('shown.bs.modal', function(){
    setTimeout(function() { $popup.find('.signup_input').focus(); }, 200);
  });

  $popup.find('.close_btn').on('click', function(){ 
    $popup.modal('hide');
  });
  
  // countdown logic
  interval_id = setInterval(check_if_time_to_show, 1000);
  function check_if_time_to_show() {
    // Calculate how much time passed since we've visited the website
    var time_diff_in_seconds = (new Date().getTime() - parseInt(Cookies.get('popup_timestamp'), 10)) / 1000;

    // Are we are the end of the page ?
    var footer_offset = $(".footer").offset().top;
    var scrollCond = ($(window).scrollTop() >= footer_offset - $(window).height());

    // If more than 60 seconds passed and we scrolled more than 250 pixels, show popup
    if (time_diff_in_seconds >= 60 && $(document).scrollTop() > 250) { open_popup("time"); } 

    // If more than 20 seconds passed and we scrolled to the end of the page, show popup
    else if (time_diff_in_seconds >= 20 && scrollCond) { open_popup("scroll"); }
  }
  
  function open_popup(source) {
    if (Cookies.get('popup_shown')) return;
    clearInterval(interval_id);
    
    update_origin_source(source);

    $popup.modal('show');
    Cookies.set('popup_shown', true);
  }

  function update_origin_source(source) {
    // Get signup form element
    var $form = $popup.find('form');

    var new_origin = $form.data('origin') + "_" + source;
    var new_action = $form.attr('action') + "_" + source;
    
    $form.attr('data-origin', new_origin);
    $form.attr('action', new_action);
  }
});
</script>
